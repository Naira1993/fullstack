{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "watches, shop",
  "main": "index.js",
  "engines": {
       "node": "12.16.1",
       "npm": "6.13.4"            
  },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
